{
  "expo": {
    "name": "ClimaNota",
    "slug": "storm-chaser",
    "version": "2.53.35.826",
    "orientation": "portrait",
    "icon": "./assets/icon.png",
    "scheme": "stormchaser",
    "userInterfaceStyle": "automatic",
    "newArchEnabled": true,
    "splash": {
      "image": "./assets/splash-icon.png",
      "resizeMode": "contain",
      "backgroundColor": "#ffffff"
    },
    "ios": {
      "supportsTablet": true,
      "infoPlist": {
        "NSCameraUsageDescription": "This app needs access to camera to capture storm photos.",
        "NSLocationWhenInUseUsageDescription": "This app needs access to location to provide weather data and track storm locations.",
        "NSPhotoLibraryUsageDescription": "This app needs access to photo library to save and access storm photos."
      }
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./assets/adaptive-icon.png",
        "backgroundColor": "#ffffff"
      },
      "permissions": [
        "READ_EXTERNAL_STORAGE",
        "READ_MEDIA_IMAGES",
        "CAMERA",
        "ACCESS_FINE_LOCATION",
        "ACCESS_COARSE_LOCATION",
        "WRITE_EXTERNAL_STORAGE"
      ],
      "package": "com.omnidevxs.stormchaser",
      "edgeToEdgeEnabled": true
    },
    "web": {
      "favicon": "./assets/favicon.png"
    },
    "extra": {
      "eas": {
        "projectId": "6ee74c34-5d41-470f-bc9e-1f3912f15596"
      }
    },
    "plugins": [
      "expo-sqlite",
      "expo-camera",
      "expo-location",
      "expo-media-library"
    ]
  }
}
